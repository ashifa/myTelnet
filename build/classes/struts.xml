<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.objectFactory" value="spring" />
	<constant name="struts.action.extension" value="do" />
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="true" />

	<package name="myweb" namespace="/" extends="struts-default">


		<interceptors>
			<interceptor-stack name="myStack">
				<interceptor-ref name="tokenSession" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>
		</interceptors>

		<default-interceptor-ref name="defaultStack" />

		<default-action-ref name="index" />

		<global-results>
			<result name="error">/Error.jsp</result>
			<result name="invalid.token">/Error.jsp</result>
		</global-results>

		<global-exception-mappings>
			<exception-mapping exception="java.lang.Exception"
				result="error" />
		</global-exception-mappings>

		<action name="graphicalLog" class="graphicallog.action.GraphicalLogAction">
			<result>/LogViewer.jsp</result>
			<result name="invalid.token">/Error.jsp</result>
			<!-- <interceptor-ref name="myStack" /> -->
		</action>

		<action name="telnet" class="telnet.action.TelnetAction">
			<result>/Telnet.jsp</result>
			<result name="DEFAULT">/TelnetDefault.jsp</result>
		</action>

		<action name="test" class="telnet.action.Test">
			<result>/Test.jsp</result>
		</action>



	</package>


</struts>
